doctype html
html(lang="en")
    head
        title Anya Novikova | Top Upwork Developer, responsive HTML5, Jade, Javascript, Django

        meta(charset="utf-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        meta(name="description", content="Top rated 10% Upwork HTML5, CSS3, Jade, Javascript and Django templates web developer")
        meta(name="keywords", content="960grid, adaptive, ajax, angular, bootstrap, css, css3, code, developer, design, django, flat, foundation, framework, freelancer, frontend, git, github, hg, html, html5, jquery, javascript, jade, jinja2, landing, layout, less, mobile, nodejs, nginx, oDesk, parallax, photoshop, project, psd, responsive, sass, sketch, stylesheet, twitterbootstrap, template, ui, userinterface, userexperience, upwork, web")

    body
        .container.parallax_container
            .design-copy Menangen design
            ul#scene
                -
                    var layer1 = {":src": "layer1Src"}
                    var layer2 = {":src": "layer2Src"}
                    var layer3 = {":src": "layer3Src"}
                    var layer4 = {":src": "layer4Src"}
                    var layer5 = {":src": "layer5Src"}
                li.layer(data-depth="0.00"): img.cloud_layer&attributes(layer5)
                li.layer(data-depth="0.10"): img.cloud_layer&attributes(layer1)
                li.layer(data-depth="0.30"): img.cloud_layer.cloud_two&attributes(layer2)
                li.layer(data-depth="0.60"): img.cloud_layer.cloud_three&attributes(layer3)
                li.layer(data-depth="1.00"): img.cloud_layer.cloud_four&attributes(layer4)

        .container.image_container

            .row
                .col-md-12.avatar.text-center
                    img.img-circle.img-avatar(src="images/avatar.jpg", width="120px", height="120px", alt="Anya Novikova photo", title="Anya Novikova")
            .row
                .col-md-12.margined-top-div.text-center
                    span(title="TOP 10 Upwork HTML5 Developer").name-header.centered-scale-element HTML5 Jade Javascript developer Anya Novikova
            .row
                .col-md-6.margined-top-div.text-center
                    - var mailView = {"v-text": "email"}
                    a#mail: span(title="Send me email").email.centered-scale-element&attributes(mailView)
                .col-md-6.margined-top-div.text-center
                    span(alt="Skype login", title="My login in Skype").skype-header.left skype
                    span(alt="Skype login", title="My login in Skype").skype-header.center :
                    span(alt="Skype login", title="My login in Skype").skype-header.right ane4kanoka


            .row
                .tags.col-md-12.text-center#tags
                    template#child-template
                        - var vueTagComponent = {"@mouseover": "mouseOver", "@mouseout": "mouseOut", "@click": "click", ":class": "{ 'open': tags[tagname]}"}
                        a(href="#counter")&attributes(vueTagComponent)

                            - var size = 96
                            - var vueImage = {":class": "lessClass", "alt": "{{* tagname}}", "title": "{{* tagname}}"}

                            img(
                            src="https://upload.wikimedia.org/wikipedia/commons/c/ce/Transparent.gif",
                            width="#{size}px", height="#{size}px"
                            )&attributes(vueImage)

                    // Django tag
                    tag(tagname="django")
                    // Twitter Bootstrap tag
                    tag(tagname="bootstrap")
                    // Less framework tag
                    tag(tagname="less")
                    // Sass / SCSS tag
                    tag(tagname="sass")
                    // Node.js tag
                    tag(tagname="nodejs")
                    // Python tag
                    tag(tagname="python")
                    // Javascript ECMAScript 6 tag
                    tag(tagname="javascript")
                    // jQuery tag
                    tag(tagname="jquery")
                    // Angular tag
                    tag(tagname="angular")

            .row
                .col-md-12.margined-top-div.text-center.text-uppercase#counter {{ projectsCounterText }}

            .row
                .col-md-10.col-md-offset-1.margined-top-div#projectsList
                    //- Display filtered projects in loop
                    - var groupsAttributes = {"v-for": "couple in group"}
                    .row&attributes(groupsAttributes)

                        - var coupleAttributes = {"v-for": "project in couple"}
                        .col-md-6.col-sm-6.project&attributes(coupleAttributes)
                            .bordered-block
                                .row
                                    .col-md-12
                                        span.header.text-center {{ project.name }}
                                .row
                                    .col-md-4.col-sm-12.project-thumbnail
                                        .row
                                            .col-md-12
                                                img.center-block.img-responsive(src="images/portfolio-thumb/{{ project.image }}")
                                        .row.center-block
                                            .col-md-12.col-sm-12.col-xs-12.tags-container
                                                .tag.text-center
                                                    span.label.label-tag django
                                                .tag.text-center
                                                    span.label.label-tag python
                                                .tag.text-center
                                                    span.label.label-tag angular
                                                .tag.text-center
                                                    span.label.label-tag jquery
                                                .tag.text-center
                                                    span.label.label-tag javascript

                                    .col-md-8.col-sm-12.description {{{ project.description }}}
            .row
                nav.col-md-4.col-md-offset-4.text-center.flag-language
                    a(href="http://ane4k.in")
                        img(src="images/flag-russian.svg", alt="russian site", title="russian", width="48px")
                        span(data-hover="RUSSIAN") RUSSIAN

        //-TODO:remove font awesomelink(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css")
        link(rel="stylesheet", href="css/main.min.css")

        //- Concatenated by gulp task Javascript: js/vendor/src/vue.min.js + js/vendor/src/zepto.min.js custom build (core only) + js/vendor/src/parallax.min.js
        script(type="text/javascript", src="js/all.js", async="async")
        //- Google Analytics script
        script(type='text/javascript').
            window._gaq = window._gaq || [];
            window._gaq.push(['_setAccount', 'UA-50636137-1']);
            window._gaq.push(['_trackPageview']);
        //- script(type="text/javascript", src="http://www.google-analytics.com/ga.js", async="async")